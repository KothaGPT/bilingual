{
  "model_type": "gpt2",
  "architecture": "GPT2LMHeadModel",
  "vocab_size": 32000,
  "n_positions": 512,
  "n_ctx": 512,
  "n_embd": 512,
  "n_layer": 8,
  "n_head": 8,
  "activation_function": "gelu_new",
  "resid_pdrop": 0.1,
  "embd_pdrop": 0.1,
  "attn_pdrop": 0.1,
  "layer_norm_epsilon": 1e-5,
  "initializer_range": 0.02,
  "bos_token_id": 1,
  "eos_token_id": 2,
  "style_tokens": {
    "formal": "<|formal|>",
    "informal": "<|informal|>",
    "poetic": "<|poetic|>",
    "literary": "<|literary|>"
  },
  "task_specific_params": {
    "style-transfer": {
      "do_sample": true,
      "max_length": 256,
      "temperature": 0.7,
      "top_p": 0.85,
      "repetition_penalty": 1.2
    }
  }
}
